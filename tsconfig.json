  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
  }
